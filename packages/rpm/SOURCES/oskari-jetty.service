[Unit]
Description=Oskari Server (Jetty)
After=network.target

[Service]
Type=simple
WorkingDirectory=/usr/share/jetty
Environment="NAME="
EnvironmentFile=/etc/sysconfig/oskari-jetty
ExecStart=/usr/bin/java -cp /usr/share/jetty/resources:/usr/share/jetty/lib/* $JAVA_OPTIONS org.oskari.jetty.JettyServer

SuccessExitStatus=143
User=jetty
Group=jetty

Restart=on-failure
RestartSec=30

[Install]
WantedBy=multi-user.target
